(version 1)

(depends
 (choice
  ((((arch x86_64) (os win32) (os-distribution cygwin) (os-family windows)))
   (ocaml-env-mingw64))
  ((((arch arm64) (os win32) (os-distribution cygwin) (os-family windows)))
   (ocaml-env-mingw32))))

(enabled_on_platforms
 (only
  ((arch x86_64)
   (os win32)
   (os-distribution cygwin)
   (os-family windows))
  ((arch arm64)
   (os win32)
   (os-distribution cygwin)
   (os-family windows))))
